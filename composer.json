{
    "name": "guesttoguest/neo4j-ogm-module",
    "description": "Neo4jPHP OGM Module for Zend Framework 2",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.4.*",
        "doctrine/doctrine-orm-module": "0.*",
        "guesttoguest/neo4jphp": "dev-master",
        "guesttoguest/neo4jphp-ogm": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Everyman\\Neo4j":"lib/",
            "HireVoice": "lib/",
            "Neo4jOGMModule": "src/"
        }
    },
    "repositories": [
        { 
            "type": "vcs",
            "url": "https://github.com/GuestToGuestTeam/neo4jphp"
        },
        { 
            "type": "vcs",
            "url": "https://github.com/GuestToGuestTeam/Neo4j-PHP-OGM"
        }
    ]
}
